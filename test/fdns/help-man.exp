set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "fdns --help\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"fdns - DNS over HTTPS proxy server"
}
after 100

send -- "fdns -?\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"fdns - DNS over HTTPS proxy server"
}
after 100

send -- "fdns --version\r"
expect {
	timeout {puts "TESTING ERROR 2\n";exit}
	"FDNS version"
}
after 100

send -- "rm -f tmp\r"
after 100
send -- "man fdns > tmp\r"
sleep 2
send -- "cat tmp\r"
expect {
	timeout {puts "TESTING ERROR 3\n";exit}
	"fdns - Firejail DNS over HTTPS/TLS proxy"
}
after 100
send -- "rm -f tmp"
after 100
send -- "pkill fdns\r"

after 100
puts "\nall done\n"

